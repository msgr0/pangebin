//global_defaults

trace.fields = 'task_id,name,status,exit,realtime,%cpu,rss'
manifest.defaultBranch = 'main'

params
{
    input = null
    dataset = null
    output = null
    ground_truth = false
    plasgraph = null
}

profiles {
    tower {
        trace.enabled = true
    }

    gt {
        params {
            ground_truth = true
        }
    }

    test {
        params {
            input = "$projectDir/test_data/ecol-SAMN04014855"
            output = "$projectDir/test_output/"

        }
    }

    test_set {
        params {
            dataset = "$projectDir/test_data/"
            output = "$projectDir/test_output/"
        }
    }
}
